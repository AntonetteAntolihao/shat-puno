<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="476.0" prefWidth="353.0" spacing="20.0" style="-fx-background-color: magenta;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo8.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Login" textFill="WHITE">
      <font>
         <Font name="System Bold Italic" size="33.0" />
      </font>
      <VBox.margin>
         <Insets bottom="-24.0" />
      </VBox.margin>
   </Label>
   <Label text="Please enter you Login and your Password" textFill="WHITE">
      <font>
         <Font name="System Bold Italic" size="13.0" />
      </font>
      <VBox.margin>
         <Insets bottom="41.0" />
      </VBox.margin>
   </Label>
   <PasswordField promptText="Username or E-mail">
      <VBox.margin>
         <Insets bottom="-9.0" />
      </VBox.margin>
   </PasswordField>
   <PasswordField promptText="Password">
      <VBox.margin>
         <Insets bottom="-18.0" />
      </VBox.margin>
   </PasswordField>
   <Button mnemonicParsing="false" style="-fx-background-color: transparent;" text="Forgot password" textFill="WHITE">
      <VBox.margin>
         <Insets bottom="9.0" left="205.0" />
      </VBox.margin>
   </Button>
   <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="306.0" style="-fx-background-color: transparent; -fx-border-color: white;" text="Login" textFill="WHITE">
      <font>
         <Font name="System Bold" size="13.0" />
      </font>
      <VBox.margin>
         <Insets bottom="-49.0" />
      </VBox.margin>
   </Button>

    <Label fx:id="welcomeText" />
   <Button mnemonicParsing="false" prefHeight="27.0" prefWidth="305.0" style="-fx-background-color: transparent; -fx-border-color: white;" text="Or sign-in with Google" textFill="WHITE">
      <font>
         <Font name="System Bold" size="13.0" />
      </font>
      <VBox.margin>
         <Insets bottom="-13.0" />
      </VBox.margin>
   </Button>
    <Button onAction="#onHelloButtonClick" style="-fx-background-color: transparent;" text="Not a member yet? Register" textFill="WHITE" />
</VBox>
